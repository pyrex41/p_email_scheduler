# Email Scheduler Environment Variables Example
# Copy this file to .env and modify as needed

# SendGrid API Configuration
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=medicare@example.com
FROM_NAME=Medicare Services

# Email Sending Control
# Set to "false" to actually send emails, "true" for dry-run mode (default)
EMAIL_DRY_RUN=true

# Email Sending Mode Control
# Controls whether emails are sent in test/production modes
# Set to "ENABLED" or "DISABLED"
TEST_EMAIL_SENDING=ENABLED
PRODUCTION_EMAIL_SENDING=DISABLED

# Application Configuration
EMAIL_SCHEDULER_BASE_URL=https://maxretain.com
QUOTE_SECRET=your_quote_secret_here
LOG_FILE=logs/email_scheduler.log